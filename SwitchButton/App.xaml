<Application x:Class="SwitchButton.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SwitchButton"
             StartupUri="Switcher.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\Background.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles\Rectangle.xaml" ></ResourceDictionary>
                <ResourceDictionary Source="Styles\Circle.xaml"    ></ResourceDictionary>
                <ResourceDictionary Source="Styles\Effect.xaml"    ></ResourceDictionary>
                <ResourceDictionary Source="Styles\Font.xaml"      ></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
