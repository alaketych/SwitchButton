<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SwitchButton.Styles">
    
    <Style x:Name="CircleBackgorund" TargetType="Ellipse">
        <Setter Property="Fill" Value="{StaticResource Circle}"></Setter>
        <Setter Property="Fill" Value="{StaticResource CircleBehind}"></Setter>
        <Setter Property="Fill" Value="{StaticResource CirclePressed}"></Setter>
    </Style>
    
</ResourceDictionary>